# 25197
./run_gren_pairwise 25197-er-uninfected 25197-er-infected /home/abrehm/GWAS2/new_seq/results/trimmed_fastq 

./freq_to_fisher_p grenedalf.frequency.25197-er-uninfected_25197-er-infected.frequency.csv > grenedalf.frequency.25197-er-uninfected_25197-er-infected.frequency.csv.neg_log_pval


# FoCo17 merged
./run_gren_pairwise Foco17Positive_merged Foco17Negative_merged /home/abrehm/GWAS2/new_seq/results/trimmed_fastq & 

# FoCo23 merged
./run_gren_pairwise Foco23Positive Foco23Negative_merged /home/abrehm/GWAS2/new_seq/results/trimmed_fastq &

wait

./freq_to_fisher_p grenedalf.frequency.Foco17Positive_merged_Foco17Negative_merged.frequency.csv > grenedalf.frequency.Foco17Positive_merged_Foco17Negative_merged.frequency.csv.neg_log_pval

./freq_to_fisher_p grenedalf.frequency.Foco23Positive_Foco23Negative_merged.frequency.csv > grenedalf.frequency.Foco23Positive_Foco23Negative_merged.frequency.csv.neg_log_pval



# wild 
./run_gren_pairwise A2 Negative_combined /home/abrehm/GWAS2/new_seq/results/trimmed_fastq   

./freq_to_fisher_p grenedalf.frequency.A2_Negative_combined.frequency.csv > grenedalf.frequency.A2_Negative_combined.frequency.csv.neg_log_pval

exit



